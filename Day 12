# Python program to illustrate
# Append vs write mode
file1 = open("30 days 30 hours Operations.txt", "w")
L = ["I have completed 10 days successful"]
file1.writelines(L)
file1.close()
   
# Append-adds at last
file1 = open("30 days 30 hours Operations", "a")  # append mode
file1.write("VAISHNAVI \n")
file1.close()
   
file1 = open("30 days 30 hours operations.txt", "r")
print("Output of Readlines after appending")
print(file1.read())
print()
file1.close
